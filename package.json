  "repository": "git@github.com:dennisseidel/react-starter.git",
  "engines": {
    "node": "7.4.0"
  },
  "scripts": {
    "start": "node server/server.js",
    "build": "NODE_ENV=production webpack -p --config ./webpack.production.config.js",
